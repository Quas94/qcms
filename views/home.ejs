<!-- Front page -->
<div class="no-padding-div" id="page-home">
    <div class="col-md-9 pull-right" ng-repeat="post in posts">
        <div class="panel panel-default panel-main">
            <div class="panel-heading">
                <h3 class="panel-title">{{post.title}}</h3>
            </div>
            <!-- panel-body-wrapper div necessary to prevent jumpy animation of collapse due to panel padding -->
            <div class="no-padding-div">
                <div class="panel-body">
                    <p>{{post.body}}</p>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End front page -->
